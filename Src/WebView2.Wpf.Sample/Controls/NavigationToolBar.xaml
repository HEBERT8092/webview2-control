<UserControl x:Class="MtrDev.WebView2.Wpf.Sample.Controls.NavigationToolBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MtrDev.WebView2.Wpf.Sample.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="Auto" />
        </Grid.ColumnDefinitions>
        <Button x:Name="buttonBack" Grid.Column="0" Margin="2" Click="buttonBack_Click">Back</Button>
        <Button x:Name="buttonForward" Grid.Column="1" Margin="2" Click="buttonForward_Click">Forward</Button>
        <Button x:Name="buttonReload" Grid.Column="2" Margin="2" Click="buttonReload_Click">Reload</Button>
        <Button x:Name="buttonCancel" Grid.Column="3" Margin="2" Click="buttonCancel_Click">Cancel</Button>
        <TextBox x:Name="textBoxUrl" Grid.Column="4"  Margin="2"/>
        <Button Grid.Column="5" Margin="2" Click="buttonGo_Click">Go</Button>
    </Grid>
</UserControl>
