<Window x:Class="MtrDev.WebView2.Wpf.Sample.Forms.AboutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MtrDev.WebView2.Wpf.Sample.Forms"
        mc:Ignorable="d"
        Title="AboutWindow" Height="150" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" Margin="5">WebView2APISample, Version 1.0</TextBlock>
        <TextBlock Grid.Row="1" Margin="5">Copyright (C) 2019</TextBlock>
        <Button Grid.Row="2" Width="35" Height="25" Margin="5" Click="Button_Click">Ok</Button>
    </Grid>
</Window>
